<%= form_for([@deal, @deal.customers.build]) do |f| %>
  <%= f.label :name %>  
  <%= f.text_field :name %></br>
  <%= f.label :email_id %>  
  <%= f.text_field :email_id %></br>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %></br>
  <%= f.label :address %>
  <%= f.text_field :address %></br>
  <%= f.label :credit_card_number %>
  <%= f.text_field :credit_card_number %></br>

  <%= f.submit "submit" %>

<% end %>